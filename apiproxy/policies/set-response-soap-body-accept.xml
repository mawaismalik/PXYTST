<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<AssignMessage async="false" continueOnError="false" enabled="true" name="set-response-soap-body-accept">
    <DisplayName>Set Response SOAP Body Accept</DisplayName>
    <Set>
        <Payload contentType="{acceptformat}">{soapresponse.body}</Payload>
    </Set>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
    <AssignTo createNew="false" transport="http" type="response"/>
</AssignMessage>
