<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ExtractVariables async="false" continueOnError="false" enabled="true" name="get-response-soap-body-xml">
    <DisplayName>Get Response SOAP Body XML</DisplayName>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
    <XMLPayload stopPayloadProcessing="false">
        <Variable name="body" type="nodeset">
            <XPath>/Envelope/Body/*[1]</XPath>
        </Variable>
    </XMLPayload>
    <Source>response</Source>
    <VariablePrefix>soapresponse</VariablePrefix>
</ExtractVariables>